<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="http://www.fpt.edu.vn/project/car">
    <xs:element name="Car">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Driver" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="numberOfSeats" type="xs:int" default="24" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
            <xs:attribute name="numberPlate" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
